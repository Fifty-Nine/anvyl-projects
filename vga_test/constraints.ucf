NET "TAP[4]"  LOC = U10  | IOSTANDARD=LVCMOS33;
NET "TAP[5]"  LOC = Y8   | IOSTANDARD=LVCMOS33;
NET "TAP[6]"  LOC = U9   | IOSTANDARD=LVCMOS33;
NET "TAP[7]"  LOC = Y9   | IOSTANDARD=LVCMOS33;
NET "TAP[8]"  LOC = Y12  | IOSTANDARD=LVCMOS33;
NET "TAP[9]"  LOC = W10  | IOSTANDARD=LVCMOS33;
NET "TAP[10]" LOC = R11  | IOSTANDARD=LVCMOS33;
NET "TAP[11]" LOC = T10  | IOSTANDARD=LVCMOS33;
NET "TAP[12]" LOC = V9   | IOSTANDARD=LVCMOS33;
NET "TAP[13]" LOC = AA8  | IOSTANDARD=LVCMOS33;
NET "TAP[14]" LOC = W9   | IOSTANDARD=LVCMOS33;
